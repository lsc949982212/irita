{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "",
  "properties": {
    "basicInfo":{
      "$id": "#/properties/basicInfo",
      "type": "object",
      "title": "基本信息",
      "required": ["assetNo", "assetType","assetName"],
      "properties": {
        "assetNo": {
          "$id": "#/properties/basicInfo/properties/assetNo",
          "type": "string",
          "title": "资产编码",
          "default": "",
          "examples": ["CDCD::8475:1111:3900:2020"],
          "pattern": "^(.*)$"
        },
        "assetType": {
          "$id": "#/properties/basicInfo/properties/assetType",
          "type": "string",
          "title": "资产分类",
          "default": "",
          "examples": ["receivableSchema"],
          "pattern": "^(.*)$"
        },
        "assetName": {
          "$id": "#/properties/basicInfo/properties/assetName",
          "type": "string",
          "title": "资产名称",
          "default": "",
          "examples": ["应收账款1"],
          "pattern": "^(.*)$"
        },
        "assetOwner": {
          "$id": "#/properties/basicInfo/properties/assetOwner",
          "type": "string",
          "title": "所有者",
          "default": "",
          "examples": ["A公司"],
          "pattern": "^(.*)$"
        }
      }
    },
    "detailedInfo":{
      "$id": "#/properties/detailedInfo",
      "type": "object",
      "title": "资产详情",
      "properties": {
        "receivable": {
          "$id": "#/properties/detailedInfo/properties/receivable",
          "type": "object",
          "title": "",
          "properties": {
            "cargoName": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/cargoName",
              "type": "string",
              "title": "交易货物名称",
              "default": "",
              "examples": ["string"],
              "pattern": "^(.*)$"
            },
            "firstPaymentDate": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/firstPaymentDate",
              "type": "string",
              "title": "首次收款日期",
              "default": "",
              "examples": ["2019-10-01"],
              "format": "date"
            },
            "endPaymentDate": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/endPaymentDate",
              "type": "string",
              "title": "末次收款日期",
              "default": "",
              "examples": ["2020-03-01"],
              "format": "date"
            },
            "lastPaymentDate": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/lastPaymentDate",
              "type": "string",
              "title": "最晚收款日",
              "default": "",
              "examples": ["2020-06-01"],
              "format": "date"
            },
            "allowOverdue": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/allowOverdue",
              "type": "boolean",
              "title": "是否允许逾期",
              "default": false,
              "examples": [true]
            },
            "overdueInterestRate": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/overdueInterestRate",
              "type": "number",
              "title": "逾期利率",
              "default": 0.0,
              "examples": [0.05]
            },
            "payerCode": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/payerCode",
              "type": "string",
              "title": "付款人代码",
              "default": "",
              "examples": ["string"],
              "pattern": "^(.*)$"
            },
            "payerName": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/payerName",
              "type": "string",
              "title": "付款人名称",
              "default": "",
              "examples": ["string"],
              "pattern": "^(.*)$"
            },
            "paymentMethod": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/paymentMethod",
              "type": "string",
              "title": "付款方式",
              "default": "现金/现汇",
              "examples": ["现金/现汇"],
              "enum": ["现金/现汇","商业承兑汇票","银行承兑汇票"]
            },
            "receivableTotalAmount": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/receivableTotalAmount",
              "type": "number",
              "title": "应收款总额",
              "default": 0.0,
              "examples": [1234.45]
            },
            "currency": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/currency",
              "type": "string",
              "title": "币种",
              "default": "CNY",
              "examples": ["CNY"],
              "enum":["CAD","AUD","CHF","CNH","GBP","HKD","SGD","CNY","EUR","JPY","USD","SEK","NOK","DKK"]
            },
            "comments": {
              "$id": "#/properties/detailedInfo/properties/receivable/properties/comments",
              "type": "string",
              "title": "应收账款摘要与说明",
              "default": "",
              "examples": ["string"],
              "pattern": "^(.*)$"
            }
          }
        },
        "payments": {
          "$id": "#/properties/detailedInfo/properties/payments",
          "type": "array",
          "title": "款项信息",
          "items": {
            "$id": "#/properties/detailedInfo/properties/payments/items",
            "type": "object",
            "title": "",
            "properties": {
              "paymentDate": {
                "$id": "#/properties/detailedInfo/properties/payments/items/properties/paymentDate",
                "type": "string",
                "title": "收款日期",
                "default": "",
                "examples": ["2019-10-01"],
                "format": "date"
              },
              "amount": {
                "$id": "#/properties/detailedInfo/properties/payments/items/properties/amount",
                "type": "number",
                "title": "金额",
                "default": 0.0,
                "examples": [1234.45]
              },
              "stage": {
                "$id": "#/properties/detailedInfo/properties/payments/items/properties/stage",
                "type": "string",
                "title": "收款阶段",
                "default": "",
                "examples": ["已签合同"],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "contracts": {
          "$id": "#/properties/detailedInfo/properties/contracts",
          "type": "array",
          "title": "合同信息",
          "items": {
            "$id": "#/properties/detailedInfo/properties/contracts/items",
            "type": "object",
            "title": "",
            "properties": {
              "contractNo": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/contractNo",
                "type": "string",
                "title": "合同编号",
                "default": "",
                "examples": ["UUID string"],
                "pattern": "^(.*)$"
              },
              "originalContractNo": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/originalContractNo",
                "type": "string",
                "title": "原始合同号",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "contractName": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/contractName",
                "type": "string",
                "title": "合同名称",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "contractType": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/contractType",
                "type": "string",
                "title": "合同类型",
                "default": "购销合同",
                "examples": ["购销合同"],
                "enum":["购销合同","框架协议","其他"]
              },
              "payerCode": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/payerCode",
                "type": "string",
                "title": "付款人代码",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "payerName": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/payerName",
                "type": "string",
                "title": "付款人名称",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "payerBankName": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/payerBankName",
                "type": "string",
                "title": "付款人开户行",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "payerBankAccount": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/payerBankAccount",
                "type": "string",
                "title": "付款人账户",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "receiverCode": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/receiverCode",
                "type": "string",
                "title": "收款人代码",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "receiverName": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/receiverName",
                "type": "string",
                "title": "收款人名称",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "receiverBankName": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/receiverBankName",
                "type": "string",
                "title": "收款人开户行",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "receiverBankAccount": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/receiverBankAccount",
                "type": "string",
                "title": "收款人账户",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "contractAmount": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/contractAmount",
                "type": "number",
                "title": "合同金额",
                "default": 0.0,
                "examples": [12345.12]
              },
              "currency": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/currency",
                "type": "string",
                "title": "币种",
                "default": "",
                "examples": ["enum string"],
                "enum":["CAD","AUD","CHF","CNH","GBP","HKD","SGD","CNY","EUR","JPY","USD","SEK","NOK","DKK"]
              },
              "contractDate": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/contractDate",
                "type": "string",
                "title": "签订日期",
                "default": "",
                "examples": ["2019-05-01"],
                "format": "date"
              },
              "contractAttachment": {
                "$id": "#/properties/detailedInfo/properties/contracts/items/properties/contractAttachment",
                "type": "string",
                "title": "合同附件",
                "default": "",
                "examples": ["storage URI"],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "invoices": {
          "$id": "#/properties/detailedInfo/properties/invoices",
          "type": "array",
          "title": "发票信息",
          "items": {
            "$id": "#/properties/detailedInfo/properties/invoices/items",
            "type": "object",
            "title": "",
            "properties": {
              "invoiceNo": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/invoiceNo",
                "type": "string",
                "title": "发票编号",
                "default": "",
                "examples": ["UUID string"],
                "pattern": "^(.*)$"
              },
              "invoiceDate": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/invoiceDate",
                "type": "string",
                "title": "开票日期",
                "default": "",
                "examples": ["2019-07-01"],
                "format": "date"
              },
              "invoiceAmount": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/invoiceAmount",
                "type": "number",
                "title": "发票金额",
                "default": 0.0,
                "examples": [1234.12]
              },
              "currency": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/currency",
                "type": "string",
                "title": "币种",
                "default": "",
                "examples": ["CNY"],
                "enum":["CAD","AUD","CHF","CNH","GBP","HKD","SGD","CNY","EUR","JPY","USD","SEK","NOK","DKK"]
              },
              "payerCode": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/payerCode",
                "type": "string",
                "title": "付款人代码",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "payerName": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/payerName",
                "type": "string",
                "title": "付款人名称",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "receiverCode": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/receiverCode",
                "type": "string",
                "title": "收款人代码",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "receiverName": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/receiverName",
                "type": "string",
                "title": "收款人名称",
                "default": "",
                "examples": ["string"],
                "pattern": "^(.*)$"
              },
              "invoiceAttachment": {
                "$id": "#/properties/detailedInfo/properties/invoices/items/properties/invoiceAttachment",
                "type": "string",
                "title": "发票附件",
                "default": "",
                "examples": ["storage URI"],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}