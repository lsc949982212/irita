{
  "definitions":{

  },
  "$schema":"http://json-schema.org/draft-07/schema#",
  "$id":"http://example.com/root.json",
  "type":"object",
  "title":"",
  "properties":{
    "basicInfo":{
      "$id":"#/properties/basicInfo",
      "type":"object",
      "title":"基本信息",
      "required":[
        "assetNo",
        "assetType",
        "assetName",
        "assetOwner"
      ],
      "properties":{
        "assetNo":{
          "$id":"#/properties/basicInfo/properties/assetNo",
          "type":"string",
          "title":"资产编号",
          "default":"",
          "examples":[
            "8475:1111:3900:2020"
          ],
          "pattern":"^(.*)$"
        },
        "assetType":{
          "$id":"#/properties/basicInfo/properties/assetType",
          "type":"string",
          "title":"资产类型",
          "default":"",
          "examples":[
            "inventory"
          ],
          "pattern":"^(.*)$"
        },
        "assetName":{
          "$id":"#/properties/basicInfo/properties/assetName",
          "type":"string",
          "title":"资产名称",
          "default":"",
          "examples":[
            "A电子技术有限公司电子屏幕"
          ],
          "pattern":"^(.*)$"
        },
        "assetOwner":{
          "$id":"#/properties/basicInfo/properties/assetOwner",
          "type":"string",
          "title":"所有者",
          "default":"",
          "examples":[
            "A电子技术有限公司"
          ],
          "pattern":"^(.*)$"
        }
      }
    },
    "detailedInfo":{
      "$id":"#/properties/detailedInfo",
      "type":"object",
      "title":"资产详情",
      "properties":{
        "inventory":{
          "$id":"#/properties/detailedInfo/properties/inventory",
          "type":"object",
          "title":"",
          "properties":{
            "cargoName":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/cargoName",
              "type":"string",
              "title":"商品名称",
              "default":"",
              "examples":[
                "手机屏幕"
              ],
              "pattern":"^(.*)$"
            },
            "cargoValue":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/cargoValue",
              "type":"integer",
              "title":"商品价值金额",
              "default":0,
              "examples":[
                199600
              ]
            },
            "currency":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/currency",
              "type":"string",
              "title":"币种",
              "default":"",
              "examples":[
                "CNY"
              ],
              "enum":[
                "CAD",
                "AUD",
                "CHF",
                "CNH",
                "GBP",
                "HKD",
                "SGD",
                "CNY",
                "EUR",
                "JPY",
                "USD",
                "SEK",
                "NOK",
                "DKK"
              ]
            },
            "cargoSpec":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/cargoSpec",
              "type":"string",
              "title":"商品规格",
              "default":"",
              "examples":[
                "1125 x 2436, 458 PPI"
              ],
              "pattern":"^(.*)$"
            },
            "cargoUnit":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/cargoUnit",
              "type":"string",
              "title":"商品单位",
              "default":"",
              "examples":[
                "个"
              ],
              "pattern":"^(.*)$"
            },
            "cargoAmount":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/cargoAmount",
              "type":"integer",
              "title":"商品数量",
              "default":0,
              "examples":[
                998
              ]
            },
            "warehouseName":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/warehouseName",
              "type":"string",
              "title":"",
              "default":"所在仓库名称",
              "examples":[
                "上海仓库-1"
              ],
              "pattern":"^(.*)$"
            },
            "warehouseLocation":{
              "$id":"#/properties/detailedInfo/properties/inventory/properties/warehouseLocation",
              "type":"string",
              "title":"The Warehouselocation Schema",
              "default":"仓库地址",
              "examples":[
                ""
              ],
              "pattern":"^(.*)$"
            }
          }
        },
        "entryRecords":{
          "$id":"#/properties/detailedInfo/properties/entryRecords",
          "type":"array",
          "title":"入库记录",
          "items":{
            "$id":"#/properties/detailedInfo/properties/entryRecords/items",
            "type":"object",
            "title":"",
            "properties":{
              "entryTime":{
                "$id":"#/properties/detailedInfo/properties/entryRecords/items/properties/entryTime",
                "type":"string",
                "title":"入库时间",
                "default":"",
                "examples":[
                  "2019-10-01 13:21:31"
                ],
                "pattern":"^(.*)$"
              },
              "cargoAmount":{
                "$id":"#/properties/detailedInfo/properties/entryRecords/items/properties/cargoAmount",
                "type":"integer",
                "title":"商品数量",
                "default":0,
                "examples":[
                  700
                ]
              },
              "cargoPrice":{
                "$id":"#/properties/detailedInfo/properties/entryRecords/items/properties/cargoPrice",
                "type":"integer",
                "title":"商品入库时单价",
                "default":0,
                "examples":[
                  200
                ]
              },
              "currency":{
                "$id":"#/properties/detailedInfo/properties/entryRecords/items/properties/currency",
                "type":"string",
                "title":"币种",
                "default":"",
                "examples":[
                  "CNY"
                ],
                "pattern":"^(.*)$",
                "enum":[
                  "CAD",
                  "AUD",
                  "CHF",
                  "CNH",
                  "GBP",
                  "HKD",
                  "SGD",
                  "CNY",
                  "EUR",
                  "JPY",
                  "USD",
                  "SEK",
                  "NOK",
                  "DKK"
                ]
              },
              "operator":{
                "$id":"#/properties/detailedInfo/properties/entryRecords/items/properties/operator",
                "type":"string",
                "title":"经办人",
                "default":"",
                "examples":[
                  "张三"
                ],
                "pattern":"^(.*)$"
              },
              "entryFile":{
                "$id":"#/properties/detailedInfo/properties/entryRecords/items/properties/entryFile",
                "type":"string",
                "title":"入库单",
                "default":"",
                "examples":[
                  "https://aliyun/entryStorageURL"
                ],
                "pattern":"^(.*)$"
              }
            }
          }
        },
        "checkRecords":{
          "$id":"#/properties/detailedInfo/properties/checkRecords",
          "type":"array",
          "title":"盘点记录",
          "items":{
            "$id":"#/properties/detailedInfo/properties/checkRecords/items",
            "type":"object",
            "title":"",
            "properties":{
              "checkTime":{
                "$id":"#/properties/detailedInfo/properties/checkRecords/items/properties/checkTime",
                "type":"string",
                "title":"盘点时间",
                "default":"",
                "examples":[
                  "2019-11-09 17:38:29"
                ],
                "pattern":"^(.*)$"
              },
              "accountAmount":{
                "$id":"#/properties/detailedInfo/properties/checkRecords/items/properties/accountAmount",
                "type":"integer",
                "title":"账存数",
                "default":0,
                "examples":[
                  1000
                ]
              },
              "realAmount":{
                "$id":"#/properties/detailedInfo/properties/checkRecords/items/properties/realAmount",
                "type":"integer",
                "title":"实存数",
                "default":0,
                "examples":[
                  998
                ]
              },
              "operator":{
                "$id":"#/properties/detailedInfo/properties/checkRecords/items/properties/operator",
                "type":"string",
                "title":"盘点数",
                "default":"",
                "examples":[
                  "李四"
                ],
                "pattern":"^(.*)$"
              },
              "checkFile":{
                "$id":"#/properties/detailedInfo/properties/checkRecords/items/properties/checkFile",
                "type":"string",
                "title":"盘点记录文件",
                "default":"",
                "examples":[
                  "https://aliyun/entryStorageURL3"
                ],
                "pattern":"^(.*)$"
              }
            }
          }
        }
      }
    },
    "dataInteract":{
      "$id":"#/properties/dataInteract",
      "type":"array",
      "title":"数据交互定义",
      "items":{
        "$id":"#/properties/dataInteract/items",
        "type":"object",
        "title":"The Items Schema",
        "required":[
          "xPath",
          "interactType"
        ],
        "properties":{
          "xPath":{
            "$id":"#/properties/dataInteract/items/properties/xPath",
            "type":"string",
            "title":"The Xpath Schema",
            "default":"",
            "examples":[
              "x.receivable.payerCode"
            ],
            "pattern":"^(.*)$"
          },
          "interactType":{
            "$id":"#/properties/dataInteract/items/properties/interactType",
            "type":"integer",
            "title":"The Interacttype Schema",
            "default":0,
            "examples":[
              1
            ]
          }
        }
      }
    },
    "authorizationProperties":{
      "$id":"#/properties/authorizationProperties",
      "type":"array",
      "title":"The Authorizationproperties Schema",
      "items":{
        "$id":"#/properties/authorizationProperties/items",
        "type":"string",
        "title":"The Items Schema",
        "default":"",
        "examples":[
          "detailedInfo.inventory.warehouseName"
        ],
        "pattern":"^(.*)$"
      }
    },
    "secretProperties":{
      "$id":"#/properties/secretProperties",
      "type":"array",
      "title":"The Secretproperties Schema",
      "items":{
        "$id":"#/properties/secretProperties/items",
        "type":"string",
        "title":"The Items Schema",
        "default":"",
        "examples":[
          "detailedInfo.inventory.warehouseLocation"
        ],
        "pattern":"^(.*)$"
      }
    }
  }
}